<script>
	var i = 0;
	var txt = "Hello, World! My name is Russell Corbett"; /* The text */
	var speed = 50; /* The speed/duration of the effect in milliseconds */

	function typeWriter() {
		if (i < txt.length) {
			document.getElementById("introduction")!.innerHTML += txt.charAt(i);
			i++;
			setTimeout(typeWriter, speed);
		}
	}

	var intro = false;
	window.addEventListener(
		"scroll",
		function (event) {
			if (isInViewport(document.getElementById("introduction")) && intro === false) {
				intro = true;
				typeWriter();
			}
		},
		false
	);

	function isInViewport(element: HTMLElement | null) {
		const bounding = element!.getBoundingClientRect();
		if (bounding.bottom <= (window.innerHeight || document.documentElement.clientHeight)) {
			return true;
		} else {
			return false;
		}
	}

	// Icon Slideshow
	var iconContainer = document.querySelector(".icon-container");
	var containerWidth = iconContainer.offsetWidth;
	var icon = document.querySelector(".icons");
	icon.style.transform = `translateX(-${containerWidth}px)`;
	icon.addEventListener("animationiteration", function () {
		var currentPosition = parseInt(icon.style.transform.replace("translateX(", "").replace(")", ""));
		var newPosition = currentPosition - containerWidth / 2;
		icon.style.transform = `translateX(${newPosition}px)`;
	});

	// Find all buttons with the `non-engineering` class on the page.
	const non_eng_button = document.querySelectorAll("button.non-engineering");
	const other_work = document.getElementById("non-engineering");
	var totalHeight = document.body.scrollHeight;
	if (other_work != null) {
		other_work.style.display = "none";
		other_work.style.opacity = 0;
		var height = other_work.offsetHeight;
		var new_height = height + totalHeight;
	}

	// Handle clicks on each button.
	non_eng_button.forEach((button) => {
		button.addEventListener("click", () => {
			if (other_work != null) {
				if (other_work.style.display === "none") {
					other_work.style.opacity = 0;
					other_work.style.display = "block";
					var opacity = 0;
					var intervalID = setInterval(function () {
						if (opacity < 1) {
							opacity = opacity + 0.01;
							other_work.style.opacity = opacity;
						} else {
							clearInterval(intervalID);
						}
					}, 10);
					button.innerHTML = "Hide Non-Engineering Positions";
				} else {
					other_work.style.display = "none";
					button.innerHTML = "Show Non-Engineering Positions";
				}
			}
		});
	});
</script>

